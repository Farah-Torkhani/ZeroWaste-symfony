{% extends 'base.html.twig' %}


{% block stylesheets %}
	<link rel="stylesheet" href="{{asset('./style/style.css')}}">
	<link rel="shortcut icon" href="{{asset('./assets/img/logo.svg')}}" type="image/x-icon">
{% endblock %}

{% block title %}
	{{title}}
{% endblock %}

{% block javascripts %}
	<script src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>
{% endblock %}


{% block body %}

	<div class="navigation">
		<nav class="navigation__nav">
			<a href="#">
				<img class="navigation__logo" src="{{asset('./assets/img/logo.svg')}}" alt="logo">
			</a>

			<ul class="navigation__list">
				<li class="navigation__item">
					<a href="#home" class="navigation__link">Home</a>
				</li>
				<li class="navigation__item">
					<a href="#ourValue" class="navigation__link">Our Values</a>
				</li>
				<li class="navigation__item">
					<a href="#fund" class="navigation__link">Fund</a>
				</li>
				<li class="navigation__item">
					<a href="#" class="navigation__link">Shop</a>
				</li>

				<div class="navigation__list--2">
					<li class="navigation__item">
						<a href="#" class="navigation__link">Login</a>
					</li>
					<div class="navigation__btn">
						<a href="./signup.html" class="navigation__primary-btn">Sign up</a>
					</div>
				</div>
			</ul>
		</nav>
	</div>

	<header id="home" class="header">

		<div class="header__social-media">
			<a target="_blank" href="https://www.facebook.com/">
				<img src="./assets/img/fb.svg" alt="" class="header__social-media__logo">
			</a>

			<a target="_blank" href="https://twitter.com/"><img src="{{asset('./assets/img/twitter.svg')}}" alt="" class="header__social-media__logo"></a>

			<a target="_blank" href="https://www.instagram.com/"><img src="{{asset('./assets/img/insta.svg')}}" alt="" class="header__social-media__logo"></a>
		</div>

		<div class="header__container">
			<div class="header__content">
				<div class="header__content__group">
					<h1 class="header__content__title">Zero<span class="header__content__title-span">Waste</span>
					</h1>
				</div>
				<p class="header__content__paragraph">
					The Zero Waste website is an eco-guide for users who want to consume wisely and who are concerned
																																																																																																																																																																																																																																																																				                    about ecology issues, namely the problem of garbage.
				</p>

				<a href="#" class="navigation__primary-btn navigation__primary-btn--2">Join us</a>
			</div>

			<lottie-player class="header__animation" src="{{asset('./assets/jsonAnimation/header2.json')}}" background="transparent" speed="1" loop autoplay></lottie-player>

		</div>

		<lottie-player class="header__scrollDown" src="{{asset('./assets/jsonAnimation/scrool.json')}}" background="transparent" speed="1" loop autoplay></lottie-player>

	</header>

	<main class="main">
		<section id="ourValue" class="ourValues">
			<lottie-player class="ourValues__leafAnimation ourValues__leafAnimation--1" src="{{asset('./assets/jsonAnimation/leafAnimation.json')}}" background="transparent" speed="1" loop autoplay></lottie-player>

			<lottie-player class="ourValues__leafAnimation ourValues__leafAnimation--2" src="{{asset('./assets/jsonAnimation/leafAnimation.json')}}" background="transparent" speed="1" loop autoplay></lottie-player>

			<div class="ourValues__intro">
				<h1 class="ourValues__intro__title">Our Values</h1>
				<p class="ourValues__intro__parag">Zero Waste is committed to values that improve the customer
																																																																																																																																																																																																																																																																				                    experience and better the world that we camp and recreate in.
				</p>
			</div>
			<div class="ourValues__container">

				<div class="ourValues__item">
					<lottie-player class="ourValues__item__animation" src="{{asset('./assets/jsonAnimation/recycle.json')}}" background="transparent" speed="1" loop autoplay></lottie-player>
					<h1 class="ourValues__item__title">Sustainable Materials & Practices</h1>
					<p class="ourValues__item__parag">
						Wherever possible we will use materials and
																																																																																																																																																																																																																																																																																																																								                        products that are designed for long term
																																																																																																																																																																																																																																																																																																																								                        outdoor durability, minimizing waste, and
																																																																																																																																																																																																																																																																																																																								                        maximizing the utilization of recycled
																																																																																																																																																																																																																																																																																																																								                        materials.
					</p>
				</div>

				<svg class="ourValues__item__devider" xmlns="http://www.w3.org/2000/svg" width="3" height="114.606" viewbox="0 0 3 114.606">
					<line id="Line_9" data-name="Line 9" y2="111.606" transform="translate(1.5 1.5)" fill="none" stroke="#43882B" stroke-linecap="round" stroke-width="3" stroke-dasharray="11"/>
				</svg>

				<div class="ourValues__item">
					<lottie-player class="ourValues__item__animation" src="{{asset('./assets/jsonAnimation/piggy_bank.json')}}" background="transparent" speed="1" loop autoplay></lottie-player>
					<h1 class="ourValues__item__title">helps individuals and associations</h1>
					<p class="ourValues__item__parag">
						Earning points raises users' awareness of how much they can recycle in their own lives and
																																																																																																																																																																																																																																																																																																																								                        assist them to buy sustainable items. Also he can help association.
					</p>
				</div>

			</div>
		</section>

