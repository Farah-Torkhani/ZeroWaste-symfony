<!DOCTYPE html>
<html>

	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>

	<body>
		{% block nav %}
			<div class="navigation">
				<nav class="navigation__nav">
					<a href="#">
						<img class="navigation__logo" src="{{asset('./assets/img/logo.svg')}}" alt="logo">
					</a>

					<ul class="navigation__list">
						<li class="navigation__item">
							<a href="#home" class="navigation__link">Home</a>
						</li>
						<li class="navigation__item">
							<a href="#ourValue" class="navigation__link">Our Values</a>
						</li>
						<li class="navigation__item">
							<a href="#fund" class="navigation__link">Fund</a>
						</li>
						<li class="navigation__item">
							<a href="#" class="navigation__link">Shop</a>
						</li>

						<div class="navigation__list--2">
							<li class="navigation__item">
								<a href="#" class="navigation__link">Login</a>
								{# {{path('app_login')}} #}
							</li>
							<div class="navigation__btn">
								<a href="{{path('app_registration')}}" class="navigation__primary-btn">Sign up</a>
							</div>
						</div>
					</ul>
				</nav>
			</div>
		{% endblock %}


		{% block body %}{% endblock %}

		{% block JsScript %}{% endblock %}
	</body>
</html></body></html>
