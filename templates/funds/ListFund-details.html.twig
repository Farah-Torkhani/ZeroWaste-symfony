                                d="M30.511,275.393l-10.246,3.018a6.736,6.736,0,0,1,.5,2.551v.371a3.623,3.623,0,0,1-5.107,3.3l-5.371-2.417a1.055,1.055,0,0,1,.866-1.925l5.371,2.417a1.51,1.51,0,0,0,2.131-1.377v-.371a4.567,4.567,0,0,0-2.722-4.146l-4.641-2.088a6.832,6.832,0,0,0-5.668.025L.628,277.025a1.055,1.055,0,0,0-.618.96v11.229a1.056,1.056,0,0,0,1.431,1.013l7.026-2.677,8.357,2.925a6.8,6.8,0,0,0,5.728-.568l11.369-6.695a4.3,4.3,0,0,0-3.41-7.819Z"
                                transform="translate(0 -254.841)" fill="#4a5bcf" />
                        </g>
                    </svg>
                    Donate
                </a>
            </div>
        </div>
      
    </div>
</div>

          {% block Js %} 
<script src="{{asset('./contents/js/script.js')}}"> </script>  
{% endblock %}

{% endblock %}
           