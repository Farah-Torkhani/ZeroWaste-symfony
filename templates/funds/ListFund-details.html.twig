
{% extends 'funds/fundbase.html.twig' %}
{% block stylesheets %}
   <link rel="stylesheet" href="{{asset('./contents/css/ST.css')}}">
    <link rel="stylesheet" href="{{asset('./contents/css/ST1.css')}}">
    <link rel="stylesheet" href="{{asset('./contents/css/ST2.css')}}">
	<link rel="stylesheet" href="{{asset('./style/nabilStyle.css')}}">

	<link rel="shortcut icon" href="{{asset('./assets/img/logo.svg')}}" type="image/x-icon">
	<link href="https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css" rel="stylesheet"/>
	<link rel="stylesheet" href="{{asset('./contents/css/style.css')}}">
	<link rel="stylesheet" href="{{asset('./style/nabilStyle.css')}}">
	<link rel="shortcut icon" href="{{asset('./assets/img/logo.svg')}}" type="image/x-icon">
	<link href="https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css" rel="stylesheet"/>
        <link rel="stylesheet" href="{{asset('./style/farahStyle.css')}}">

    {% endblock %}

{% block Fundrisingfront %}
    
<nav class="front__navbar front__navbar-scrolled-up">
    <div class="front__navbar-img-container">
      <img
        src="{{asset('./contents/img/logo.png')}}"
        class="front__navbar-img-container"
        alt=""
      />
    </div>

    <ul class="front__navbar__link">
      <li>Home</li>
      <li>Fundraising</li>
      <li>Shop</li>
      <li>collect</li>
    </ul>

    <div class="navbar__items">
      <div
        class="notification-icon"
        style="position: relative; top: 0.1rem"
      >
        <span class="cart-icon__span">3</span>
        <span class="navbar__item">
          <i class="bx bx-bell item"></i>
        </span>
      </div>

      <span class="dash__top-bar__fullname"> {{user.fullname}}  </span>

      <div class="dash__top-bar__img-container">
        <a href="#">
          <img
             src="{{asset('./assets/uploads/img/') ~ user.imgUrl }}"
            class="dash__top-bar__img"
            alt="profile"
          />
        </a>
      </div>
    </div>
  </nav>
  <!--end dash__navbar-->



<div class="dash-user-fund-detail">
    <div class="dash-user-home__greeting">
        <span class="dash-user-home__greeting--span-1">Fund detail |</span>
        <h1 class="dash-user-home__greeting--span-2">Hello Nabil, Are you ready to help people and the nature again ?
        </h1>
    </div>


    <div class="dash-user-fund-detail__container">
        <div class="dash-user-fund-detail__container__header">
            <h1 class="dash-user-fund-detail__container__header__title">Help Sami to Beat cancer</h1>
        </div>

        <div class="dash-user-fund-detail__container__body">
            <div class="dash-user-fund-detail__container__body__left">
                <div class="dash-user-fund-detail__container__body__left-1">
                    <div class="dash-user-fund-detail__img__container">
                        <img src="{{asset('./contents/uploads/funds/') ~ stu.imageDon }}" alt="" class="dash-user-fund-detail__img">
                    </div>

                    <div class="dash-user-fund-detail__association">
                        <img src="{{asset('./assets/img/hilal-logo.png')}}" alt=""
                            class="dash-user-fund-detail__association__img">
                        <h1 class="dash-user-fund-detail__association__name"><a href="#">Hilal Ahmar</a> is organizing
                            this
                            fundraiser.</h1>
                        <a href=""></a>
                    </div>

                    <div class="dash-user-fund-detail__date">
                        <svg class="dash-user-fund-detail__date__icon" xmlns="http://www.w3.org/2000/svg"
                            viewBox="0 0 31.096 31.096">
                            <g id="svgexport-7_1_" data-name="svgexport-7 (1)" transform="translate(-2.25 -2.25)">
